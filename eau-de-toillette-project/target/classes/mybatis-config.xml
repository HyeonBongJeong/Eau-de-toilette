<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias alias="Product"
			type="com.kkkj.eaude.domain.Product" />
	
		<typeAlias alias="Review"
			type="com.kkkj.eaude.domain.Review" />
			
		<typeAlias alias="Member"
			type="com.kkkj.eaude.domain.Member" />
		<typeAlias alias="Board"
			type="com.kkkj.eaude.domain.Board" />
		<typeAlias type="com.kkkj.eaude.domain.MainVO"
			alias="Main" />
		<typeAlias alias="Basket"
			type="com.kkkj.eaude.domain.Basket" />		
		<typeAlias alias="Purchasehistory"
			type="com.kkkj.eaude.domain.Purchasehistory" />
		<typeAlias alias="ShoppingDestination"
			type="com.kkkj.eaude.domain.ShoppingDestination" />	

	</typeAliases>
	<mappers>
		<mapper resource="mapper/main-mapper.xml" />
		<mapper resource="mapper/Review-mapper.xml" />
		<mapper resource="mapper/product-mapper.xml" />
		<mapper resource="mapper/member-mapper.xml" />
		<mapper resource="mapper/board-mapper.xml" />
		<mapper resource="mapper/Basket-mapper.xml" />
		<mapper resource="mapper/Purchasehistory-mapper.xml" />
		<mapper resource="mapper/shoppingDestination_mapper.xml" />

	</mappers>
</configuration>