<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias alias="Product"
			type="com.kkkj.eaude.domain.Product" />
	
		<typeAlias alias="Review"
			type="com.kkkj.eaude.domain.Review" />
			
		<typeAlias alias="Member"
			type="com.kkkj.eaude.domain.Member" />	
	
			
	</typeAliases>
	<mappers>
		<mapper resource="mapper/Review-mapper.xml" />
		<mapper resource="mapper/product-mapper.xml" />
		<mapper resource="mapper/member-mapper.xml" />
		
	</mappers>
</configuration>